/* RESET STYLES */
 
html,body,div,h1,h2,h3,h4,h5,h6,p,img,
dl,dt,dd,ol,ul,li,table,caption,tbody,
tfoot,thead,tr,th,td,form,fieldset,
embed,object,applet {
  margin: 0;
  padding: 0;
  border: 0;
}
 
/* BASICS */
 
html, body {
  background-color:#fff;
}
body {
  font-family:arial,sans-serif;
  color:#000;
  font-size:13px;
  color:#333;
}
 
a, a code { 
  color:#006699;
} 
 
 
a:active,
a:active code { 
  color:#f00;
} 
 
a:visited,
a:visited code { 
  color:#006699;
}
 
input, select,
textarea, option {
  font-family:inherit;
  font-size:inherit;
}
 
p {
  padding:0;
  margin:0 0 1em;
}
 
code, pre {
  color:#007000;
  font-family:monospace;
  line-height:1em;
}
 
var {
  color:#007000;
  font-style:italic;
}
 
pre {
  border:1px solid #ccc;
  background-color:#fafafa;
  padding:10px;
  margin:0 0 1em 1em;
  overflow:auto;
}
 
h1,h2,h3,h4,h5 {
  margin:1em 0;
  padding:0;
}
 
p,ul,ol,dl,dd,dt,li {
  line-height:1.3em;
}
 
ul,ol {
  margin:0 0 .8em;
  padding:0 0 0 2em;
}
 
li {
  padding:0 0 .5em;
}
 
dl {
  margin:0 0 1em 0;
  padding:0;
}
 
dt {  
  margin:0;
  padding:0;
}
 
dd {
  margin:0 0 1em;
  padding:0 0 0 2em;
}
 
li p, dd p {
  margin:1em 0 0;
}
 
li pre, li table, li img,
dd pre, dd table, dd img {
  margin:1em 0 0 1em;
}
 
li ul,
li ol {
  margin:.5em 0 0 0;
  padding: 0 0 0 2em;
}
 
dl li {
  padding:.5em 0 0 0;
}
 
dl dl,
ol dl,
ul dl {
  margin:0 0 1em;
  padding:0;
}
 
table {
  font-size:1em;
  margin:0 0 1em;
  padding:0;
  border-collapse:collapse;
  border-width:0;
  empty-cells:show;
}
 
td,th {
  border:1px solid #ccc;
  padding:6px 12px;
  text-align:left;
  vertical-align:top;
  background-color:inherit;
}
 
th {
  background-color:#dee8f1;
}
 
hr.blue {
  background-color:#DDF0F2;
  border:none;
  height:5px;
  margin:20px 0 10px;
}

/* Removes Blogger navbar */
#navbar-iframe {
   display: none !important;
}

/* Search box
-----------------------------------------------
 */

#searchcontrol {
  width : 380px;
}

#searchResults {
  margin-right: 25px;
}
#searchResults .gsc-control {
  width : 100%;
}

#searchResults .gsc-resultsbox-visible {
  border-bottom : 1px solid #e9e9e9;
  margin-bottom : 10px;
}

/* Header
-----------------------------------------------
 */

#header {
	height: 114px;
	position:fixed;
	background-color:#fff;
	width:100%;
	top:0;
	z-index:100;
	min-width:576px;
	padding:0;
	border-bottom:3px solid #94b922;
}

#headerLeft{
padding: 25px 10px;
line-height:1.3em;
background-image:url(http://developer.android.com/assets/images/bg_fade.jpg);
background-repeat:repeat-x;
}

#headerLeft img {
  border:0 none;
}

#headerRight {
  position:absolute;
  right:0;
  top:0;
  text-align:right;
}

/* Tabs in the header */
#header ul {
  list-style: none;
  margin: 6px 0 0;  
  padding: 0;
  height: 29px;
}

#header li {
  float: left;
  margin: 0px 2px 0px 0px;
  padding:0;
}

#header li a {
  text-decoration: none;
  display: block;
  background-image: url(http://developer.android.com/assets/images/bg_images_sprite.png);
  background-position: 0 -58px;
  background-repeat: no-repeat;
  color: #666;
  font-size: 13px;
  font-weight: bold;
  width: 94px;
  height: 29px;
  text-align: center;
  margin: 0px;
}

#header li a:hover {
  background-image: url(http://developer.android.com/assets/images/bg_images_sprite.png);
  background-position: 0 -29px;
  background-repeat: no-repeat;
}

#header li a span {
  position:relative;
  top:7px;
}

/* TAB HIGHLIGHTING */
#header .selected a,
#header .selected a:hover {
  background-image: url(http://developer.android.com/assets/images/bg_images_sprite.png);
  background-position: 0 0;
  background-repeat: no-repeat;
  color: #fff;
  font-weight: bold;
  cursor:default;
}

#headerLinks {
  margin:10px 10px 0 0;
  height:13px;
}

#headerLinks .text {
  text-decoration: none;
  color: #7FA9B5;
  font-size: 11px;
  vertical-align: top;
}

#headerLinks a {
  text-decoration: underline;
  color: #7FA9B5;
  font-size: 11px;
  vertical-align: top;
}

/* Outer-Wrapper
----------------------------------------------- */
#outer-wrapper {
  margin:0 auto;
  text-align:left;
  }

#content-wrapper {
	margin-top:117px;
}

#main-wrapper {
  max-width:900px;
  align: left;
  width:70%;
  float: left;
  word-wrap: break-word; /* fix for long text breaking sidebar float in IE */
  overflow: hidden;     /* fix for long non-text content breaking IE sidebar float */
  padding: 10px;
  }

#sidebar-wrapper {
  width: 240px;
  float: right;
  border-left: 1px dotted #cccccc;
  padding-left: 20px;
  word-wrap: break-word; /* fix for long text breaking sidebar float in IE */
  overflow: hidden;      /* fix for long non-text content breaking IE sidebar float */
}


/* Posts
-----------------------------------------------
 */

.post {
  margin:.5em 1.5em 1.5em .5em;
  }
.post h2 {
  font-size:1.5em;
  font-weight:bold;
  line-height:1.3em;
  color:#000000;
  font-family: Arial, sans-serif;
  margin: 10px auto 0 auto;
}

.post h2 a, .post h2 a:visited, .post h2 strong {
  display:block;
  color:#000000;
  font-weight:bold;
  text-decoration: none;
}

.post h2 strong, .post h2 a:hover {
  color:#333333;
  text-decoration: underline;
}

.post p {
  margin:0 0 .75em 0;
  line-height:1.3em;
}

.post p.author {
    font-style: italic;
}

.post ul li, .post ol li {
  line-height:1.3em;
}

.post dl dt {
  line-height:1.3em;
  margin-top: 2px;
}

.post dl dd {
  line-height:1.3em;
  margin-top: 5px;
  margin-bottom: 7px;
}

.post-footer {
  margin: .75em 0;
  color: #666666;
}

.post img {
  padding:4px;
  border:0px solid #000000;
  }
.post blockquote {
  margin:1em 0px;
  margin-left: 2.5em;
  border-left: 2.5px solid #B2B2B2;
  padding-left: 1em;
  }
.post blockquote p {
  margin:.75em 0;
  }

.post-byline {
  font-style: italic;
  margin-top: 7px;
}

.post-divider {
  border-bottom: 1px dotted #CCCCCC;
  width: 75%;
  margin: 0 auto;
}

/* Comments
----------------------------------------------- */
#comments h4 {
  margin:1em 0;
  font-weight: bold;
  line-height: 1.4em;
  text-transform:uppercase;
  letter-spacing:.2em;
  color: #000000;
  }

#comments-block {
  margin:1em 0 1.5em;
  line-height:1.6em;
  }
#comments-block .comment-author {
  margin:.5em 0;
  }
#comments-block .comment-body {
  margin:.25em 0 0;
  }
#comments-block .comment-footer {
  margin:-.25em 0 2em;
  line-height: 1.4em;
  text-transform:uppercase;
  letter-spacing:.1em;
  }
#comments-block .comment-body p {
  margin:0 0 .75em;
  }
.deleted-comment {
  font-style:italic;
  color:gray;
  }

#blog-pager-newer-link {
  float: left;
 }
 
#blog-pager-older-link {
  float: right;
 }

#blog-pager { 
  text-align: center;
 }

.feed-links {
  clear: both;
  line-height: 2.5em;
}

/* Sidebar Content
----------------------------------------------- */
.sidebar { 
  color: #000000;
  line-height: 1.25em;
 }

.sidebar ul {
  list-style:none;
  margin:0 0 0;
  padding:0 0 0;
}
.sidebar li {
  margin:0;
  padding:0 0 .25em 15px;
  text-indent:-15px;
  line-height:1.25em;
  }

.sidebar .widget, .main .widget { 
  border-bottom:1px dotted #cccccc;
  margin:0 0 1.0em;
  padding:0 0 1.0em;
 }

.sidebar h2{
  font-size: 1.1em;
}

.main .Blog { 
  border-bottom-width: 0;
}



/* Footer
----------------------------------------------- */
#footer {
  width:660px;
  clear:both;
  margin:0 auto;
  padding-top:15px;
  line-height: 1.6em;
  text-align: center;
  font-size:1em;
}

/** Page structure tweaks for layout editor wireframe */
body#layout #header { 
  margin-left: 0px;
  margin-right: 0px;
}

